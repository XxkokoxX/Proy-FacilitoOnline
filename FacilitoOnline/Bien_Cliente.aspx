            <script type="text/javascript">
                enableSiteMessageClose();
                Event.observe(window, 'load', function () {
                    oMySlides = new iSlideShow({
                        wait: 4000,
                        counter: 'counter-div-id',
                        container: 'websites-images',
                        imgWrapper: 'websites-images-item'
                    });

                    $$('.bligoo-ajax-box').each(function (item) {
                        drawAjaxBox(item);
                    });

                    if ($('login-link')) {
                        $('login-link').observe('click', function (item) {
                            if ($('login-menu').visible()) {
                                $('login-menu').hide();
                            } else {
                                $('login-menu').show();
                            }
                        } .bind(this));
                    }

                    if ($('ajax-about-link')) {
                        drawAboutLink();
                    }
                    if ($('footer-ajax-content')) {
                        drawFooterLinks();
                    }
                    if ($('login')) {
                        drawLoginLinks();
                    }
                });
                function getParameterByName(name) {
                    var match = RegExp('[?&]' + name + '=([^&]*)').exec(window.location.search);
                    return match && decodeURIComponent(match[1].replace(/\+/g, ' '));
                }

                function drawAjaxBox(box) {
                    var count = box.getAttribute('data-count');
                    var method = box.getAttribute('data-method');
                    if (method != '' && count != '') {
                        new Ajax.Request('/bligoo/ajaxproxy', {
                            method: method
					, parameters: { count: count }
					, onSuccess: function (transport, box) {
					    var object = transport.responseText.evalJSON();
					    box.update(object.full);
					    new Ajax.Request('/bligoo/ajaxproxy', {
					        method: "userMetricsCreate"
                                                   , parameters: {
                                                       'utm_source': getParameterByName('utm_source')
                                                        , 'utm_campaign': getParameterByName('utm_campaign')
                                                        , 'utm_media': getParameterByName('utm_media')
                                                        , 'landing': 'home-es'
                                                   }
					    });

					} .bindAsEventListener(this, box)
                        });
                    }
                }
                function drawAboutLink() {
                    new Ajax.Request('/bligoo/ajaxproxy', {
                        method: "drawTranslation"
				, parameters: { key: 'MenuBean.link.explore.about.article', path: 'explora' }
				, onSuccess: function (transport) {
				    $('ajax-about-link').update(transport.responseText);
				} .bindAsEventListener(this)
                    });
                }
                function drawFooterLinks() {
                    new Ajax.Request('/bligoo/ajaxproxy', {
                        method: "drawTranslation"
				, parameters: { key: 'MenuBean.text.ajax.explore.frontpage.footer', path: 'explora' }
				, onSuccess: function (transport) {
				    $('footer-ajax-content').update(transport.responseText);
				} .bindAsEventListener(this)
                    });
                }
                function drawLoginLinks() {
                    new Ajax.Request('/bligoo/ajaxproxy', {
                        method: "drawTranslation"
				, parameters: { key: 'MenuBean.link.home.loginorregister', lang: 'es' }
				, onSuccess: function (transport) {
				    $('login').update(transport.responseText);
				} .bindAsEventListener(this)
                    });
                }
